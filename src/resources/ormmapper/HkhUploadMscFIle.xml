<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "mybatis_mapper" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="MusicPlayer.mscDao.HkhuploadMscFileDao">
    <insert id="addMusicFile" parameterType="java.util.Map">
        insert  into  hkh_music (music_name,singer_name,music_mp3)values (
        #(musicName),
        #(singerName),
        #(mp3file)
        )
    </insert>
</mapper>
